{
  "name": "version-string-to-version",
  "version": "0.3.18",
  "description": "Convert a version string to a released Node.js version (by full or partial semver) or versions (by expression)",
  "keywords": [
    "node",
    "semver",
    "version",
    "versions",
    "version-string"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/version-string-to-version.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "isarray": "^2.0.5",
    "node-semvers": "^1.3.15"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
